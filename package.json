{
  "name": "@book000/twitterts",
  "version": "0.0.0",
  "description": "Twitter Unofficial Scraping API Library for TypeScript",
  "homepage": "https://www.npmjs.com/package/@book000/twitterts",
  "bugs": {
    "url": "https://github.com/book000/twitterts/issues"
  },
  "license": "MIT",
  "author": "Tomachi <tomachi@tomacheese.com>",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "git@github.com:book000/twitterts.git",
  "scripts": {
    "build": "run-s clean ctix compile generate-docs",
    "start": "ts-node -r tsconfig-paths/register ./src/main.ts",
    "generate-types": "ts-node -r tsconfig-paths/register ./src/generate-types.ts",
    "save-responses": "ts-node -r tsconfig-paths/register ./src/save-responses.ts",
    "clean": "rimraf dist output docs",
    "compile": "tsc -p tsconfig.build.json",
    "dev": "ts-node-dev --poll -r tsconfig-paths/register ./src/main.ts",
    "ctix": "ctix single -w --noBackup -o src -s false",
    "generate-docs": "typedoc --gitRevision master src/index.ts",
    "fix": "run-s fix:prettier fix:eslint ctix",
    "fix:eslint": "eslint . --ext ts,tsx --fix",
    "fix:prettier": "prettier --write src",
    "test": "jest --runInBand --passWithNoTests --detectOpenHandles --forceExit",
    "lint": "run-p -c lint:prettier lint:eslint lint:tsc",
    "lint:eslint": "eslint . --ext ts,tsx",
    "lint:prettier": "prettier --check src",
    "lint:tsc": "tsc",
    "prepare": "run-s build",
    "prepublishOnly": "run-s lint"
  },
  "dependencies": {
    "@book000/node-utils": "1.10.8",
    "genson-js": "0.0.8",
    "json-schema-to-typescript": "13.1.1",
    "otplib": "12.0.1",
    "puppeteer-core": "21.5.2",
    "twitter-d": "0.6.0"
  },
  "devDependencies": {
    "@types/jest": "29.5.10",
    "@types/node": "20.10.0",
    "@typescript-eslint/eslint-plugin": "6.12.0",
    "@typescript-eslint/parser": "6.12.0",
    "ctix": "2.3.0",
    "eslint": "8.54.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-n": "16.3.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-unicorn": "49.0.0",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "jest-expect-message": "1.1.3",
    "jsonc-parser": "3.2.0",
    "prettier": "3.1.0",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "ts-node-dev": "2.0.0",
    "typedoc": "0.25.3",
    "typescript": "5.1.6",
    "yarn-run-all": "3.1.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      ".+\\.ts$": [
        "ts-jest",
        {
          "tsconfig": "tsconfig.json"
        }
      ]
    },
    "testMatch": [
      "**/*.test.ts"
    ],
    "setupFilesAfterEnv": [
      "jest-expect-message"
    ]
  }
}